// creo i vettori che conterranno gli elementi estratti
vector ele = new vector {"1#sh", "2#sh", "3#sh", "4#sh" , "5#sh", "6#sh", "7#sh", "8#sh","9#sh","10#sh","11#sh","12#sh","Nessuna di queste#se#sf"};

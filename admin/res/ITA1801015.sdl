int r;
int Nran=23;
count=0;
opt="";
int idx=0;

for (int i=0,22)
{ 

if (i==0) {r=16;}

else 
{
system.setProperty("r"+i, "rand:"+Nran);
r = (int)system.getProperty("r"+i); 
}

//leggo il corrispondente indice
opt=indici.get(r);

sel=system.getProperty("ans320_"+opt);
if (sel==true) 
	{
	count=count+1;
	
			if (count<=12)
			{
			//leggo l'elemento estratto
			opt=brands.get(r);
			// lo aggiungo
			ele.set(idx,opt);
			
			//leggo il corrispondente indice
				cmd=indici.get(r);
				// lo aggiungo
				ind.set(idx,cmd);
			
			// salvo nel sistema una variabile che contiene l'item pescato.
			system.setProperty("Svar"+count, opt);
	
				idx=idx+1;
	
			}

	}
	
//rimuovo dal vettore originale l'elemento pescatp
brands.remove(r);
indici.remove(r);
Nran=Nran-1;	

}


